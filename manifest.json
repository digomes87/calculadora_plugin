{
    "name": "Getting Started Example",
    "version": "2.0",
    "description": "Build an Extension!",
    "permissions": ["activeTab","declarativeContent","storage"],

    "background": {
      "scripts": ["javascript.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Calculadora"
    },
     "page_action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "img/calculadora.png"
      }
    },
    "icons": {
      "16": "img/calculadora.png"
    }
  }
